{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug test 4",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/enunciado/wish",
      "args": ["${workspaceFolder}/enunciado/tests/4.in"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },

